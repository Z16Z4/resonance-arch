#!/usr/bin/env python
#Python Typing Text Effect - www.101computing.net/python-typing-text-effect/
import time,os,sys
os.system("clear")

def typingPrint(text):
  for character in text:
    sys.stdout.write(character)
    sys.stdout.flush()
    time.sleep(0.05)

def typingInput(text):
  for character in text:
    sys.stdout.write(character)
    sys.stdout.flush()
    time.sleep(0.05)
  value = input()
  return value

def clearScreen():
  os.system("clear")


typingPrint("Resonance OS installer\n\n")
os.system("sudo calamares &")
os.system("clear")



